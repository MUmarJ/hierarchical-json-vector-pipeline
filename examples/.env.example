# Qdrant Configuration
# Local: http://localhost:6333
# Qdrant Cloud: https://xyz-example.aws.cloud.qdrant.io:6333
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=  # Optional: Only needed for Qdrant Cloud

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# PostgreSQL Configuration (for Shopify example with source DB)
POSTGRES_DSN=dbname=shopify user=postgres password=your_password host=localhost

# MongoDB Configuration (optional, if using MongoDB as source)
# MONGODB_URI=mongodb://localhost:27017
# MONGODB_DATABASE=shopify

# Embedding Model Configuration
# Options: text-embedding-3-small (1536 dims), text-embedding-3-large (3072 dims)
EMBEDDING_MODEL=text-embedding-3-small

# Performance Tuning
BATCH_SIZE=100
MAX_WORKERS=4
